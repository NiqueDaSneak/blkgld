  <div class="login-form">
    <p class="auth-header">Existing Business Log In</p>
    <p class="auth-header"><%= flash[:error] %></p>

    <%= form_tag('/businesses/find') %>
    <div>
     <%= text_field_tag 'email', nil, :placeholder => "Email", :type => 'email', :class => 'input-field'  %>
   </div>
   <div>
     <%= password_field_tag 'password', nil, :placeholder => "Password", :class => 'input-field'  %>
   </div>
   <div>
     <%= submit_tag "Log In", :class => 'auth-button' %>
   </div>


 
 </div>
